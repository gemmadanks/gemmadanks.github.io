  doi = {10.1002/9781119640738.ch3},
  urldate = {2022-12-01},
  abstract = {One important question in the search for extraterrestrial intelligence (SETI) is what would the impact be to us here on Earth if we discovered that we are not alone in the universe? Even though extraterrestrial life has not been discovered yet, the potential answer to this question has been considerably speculated about due to persistent scientific and popular interest in SETI. A main goal here is to be able to better answer this question, especially if the discovery involved the firstever technosignature, which is a sign of extraterrestrial technology such as a radio or optical signal broadcast through space. Doing so can better inform us (especially those handling the scientific confirmation and public disclosure of such a discovery) of the potential benefits and risks associated with the societal impact of the discovery. And even if a discovery has not been made yet, it can indirectly inform as to the impact of our own destiny into space. To this end, a historical and sociological approach is used, where past historical events that alleged the discovery of extraterrestrial life (events such as inconclusive scientific discoveries and science fiction stories) are used as an analogy. In conjunction with this, a model of the societal impact of discovering the first confirmed technosignature is presented, based on a process representing the discovery's hypothetical diffusion into society. This is so as to understand what the extent of the impact can be of an actual discovery.},
  chapter = {3},
  isbn = {978-1-119-64073-8},
  langid = {english},
  keywords = {disclosure,discovery,exoplanets,SETI,social impact,technosignature,UFO},
  file = {/Users/gemmadanks/Zotero/storage/88BZCJ2Z/9781119640738.html}
}

@incollection{davisSearchingExtraterrestrialIntelligence2022,
  title = {Searching for {{Extraterrestrial Intelligence}} by {{Locating Potential ET Communication Networks}} in {{Space}}},

  keywords = {Astrophysics - Instrumentation and Methods for Astrophysics},
  file = {/Users/gemmadanks/Zotero/storage/B8L7ILG2/Brzycki et al. - 2022 - Setigen Simulating Radio Technosignatures for SET.pdf;/Users/gemmadanks/Zotero/storage/WEFUD2VD/2203.html}

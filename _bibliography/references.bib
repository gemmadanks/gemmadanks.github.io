  author = {{Haqq-Misra}, Jacob and Sheikh, Sofia and Lingam, Manasvi and Kopparapu, Ravi and Frank, Adam and Wright, Jason and Mamajek, Eric and Siegler, Nick and Price, Daniel and on Technosignatures, the NExSS Working Group},
